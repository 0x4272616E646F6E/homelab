# Generated by Ansible - DO NOT EDIT MANUALLY

[{{ nut_ups_name }}]
	driver = {{ nut_ups_driver }}
	port = {{ nut_ups_port }}
	desc = "{{ nut_ups_name | upper }}"
	{% if nut_ups_vendor_id is defined and nut_ups_vendor_id != '' %}
	vendorid = {{ nut_ups_vendor_id }}
	{% endif %}
	{% if nut_ups_product_id is defined and nut_ups_product_id != '' %}
	productid = {{ nut_ups_product_id }}
	{% endif %}
	{% if nut_ups_serial is defined and nut_ups_serial != '' %}
	serial = "{{ nut_ups_serial }}"
	{% endif %}
    
	# Polling interval (seconds)
	pollinterval = 5

